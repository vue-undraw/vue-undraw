{
  "name": "vue-undraw",
  "version": "0.4.4",
  "description": "A Vue component library for unDraw illustrations",
  "main": "src/index.js",
  "jsnext:main": "src/index.js",
  "module": "src/index.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "bundle": "vue-cli-service build --target lib --name vue-undraw ./src/index.js",
    "styleguide": "vue-styleguidist server",
    "styleguide:build": "vue-styleguidist build"
  },
  "keywords": [
    "vue",
    "vue.js",
    "vue-components",
    "illustrations",
    "svg",
    "unDraw"
  ],
  "peerDependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "deepmerge": "^2.1.1",
    "jest": "^23.6.0",
    "jest-vue-preprocessor": "^1.4.0",
    "rimraf": "^2.6.2",
    "vue-loader": "^15.4.1",
    "vue-styleguidist": "^1.8.5",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.17.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KaiWedekind/vue-undraw.git"
  },
  "author": "Kai Wedekind <wedekind.kai@googlemail.com> (https://www.kaiwedekind.com/)",
  "bugs": {
    "url": "https://github.com/KaiWedekind/vue-undraw/issues"
  },
  "homepage": "https://github.com/KaiWedekind/vue-undraw#readme",
  "license": "MIT"
}
